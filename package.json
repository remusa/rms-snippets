{
  "name": "rms-snippets",
  "displayName": "rms-snippets",
  "description": "Various snippets",
  "repository": {
    "type": "git",
    "url": "https://github.com/remusa/rms-snippets"
  },
  "publisher": "rms",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.52.0"
  },
  "keywords": [
    "React",
    "React JS",
    "React Snippets",
    "TypeScript",
    "JavaScript",
    "Python",
    "Elixir",
    "Markdown"
  ],
  "categories": [
    "Snippets"
  ],
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish",
    "format:prettier": "npx prettier --ignore-path .gitignore --write *.md snippets/*",
    "format": "npm run format:prettier"
  },
  "contributes": {
    "snippets": [
      {
        "language": "css",
        "path": "./snippets/css.json"
      },
      {
        "language": "elixir",
        "path": "./snippets/elixir.json"
      },
      {
        "language": "markdown",
        "path": "./snippets/markdown.json"
      },
      {
        "language": "python",
        "path": "./snippets/python.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/react.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/react.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/react-js.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/react-ts.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/jest.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/jest.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/jest.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/jest.json"
      }
    ]
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "vsce": "^1.84.0"
  }
}
